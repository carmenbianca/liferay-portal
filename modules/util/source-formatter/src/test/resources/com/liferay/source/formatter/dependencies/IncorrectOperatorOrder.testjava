	public void testTokenTypesEqueal() {
		int a = 3;

		if (a == a) {
			System.out.println("OK");
		}

		if (3 == 3) {
			System.out.println("OK");
		}

		if (-3 == -3) {
			System.out.println("OK");
		}

		if (+3 == +3) {
			System.out.println("OK");
		}

		if (a == 3) {
			System.out.println("OK");
		}

		if (a == +3) {
			System.out.println("OK");
		}

		if (a == -3) {
			System.out.println("OK");
		}

		if (3 == a) {
			System.out.println("NG");
		}

		if (+3 == a) {
			System.out.println("NG");
		}

		if (-3 == a) {
			System.out.println("NG");
		}
	}

