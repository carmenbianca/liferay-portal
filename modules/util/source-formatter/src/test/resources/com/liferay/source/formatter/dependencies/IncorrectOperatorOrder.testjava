	public void testVariableWithUnaryOperator() {
		int a = 3;

		if (a == a) {
			System.out.println("OK");
		}

		if (a == -a) {
			System.out.println("OK");
		}

		if (-a == -a) {
			System.out.println("OK");
		}

		if (-a == a) {
			System.out.println("OK");
		}

		if (a == +a) {
			System.out.println("OK");
		}

		if (+a == +a) {
			System.out.println("OK");
		}

		if (+a == a) {
