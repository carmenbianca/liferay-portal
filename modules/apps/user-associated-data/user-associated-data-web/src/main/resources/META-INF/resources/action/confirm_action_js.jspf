<aui:script>
	function <portlet:namespace />confirmAction(formId, actionURL, message) {
		if (confirm(message)) {
			var form = document.getElementById('<portlet:namespace />' + formId);

			if (form) {
				submitForm(form, actionURL);
			}
		}
	}
</aui:script>