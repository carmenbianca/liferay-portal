<liferay-ui:search-container-column-text
	name="scope"
	value="<%= LanguageUtil.get(resourceBundle, group.getScopeLabel(themeDisplay)) %>"
/>

<liferay-ui:search-container-column-text
	cssClass="table-cell-content"
	name="name"
	value="<%= action.getName(locale) %>"
/>

<liferay-ui:search-container-column-text
	cssClass="table-cell-content"
	name="description"
	value="<%= action.getDescription(locale) %>"
/>

<liferay-ui:search-container-column-text
	name="type"
	translate="<%= true %>"
	value="<%= action.getType() %>"
/>

<liferay-ui:search-container-column-date
	name="modified-date"
	value="<%= action.getModifiedDate() %>"
/>

<liferay-ui:search-container-column-jsp
	path="/action_actions.jsp"
/>